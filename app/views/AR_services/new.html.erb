<h1>New service</h1>

<% form_for(@service) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :service %><br />
    <%= f.text_field :servicename %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url%>
  </p>
  <p>

    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', services_path %>
