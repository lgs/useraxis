<h1>New service</h1>

<% form_for @service, new_service_path(@service)  do |f| %>

  <%= f.error_messages %>

  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :service %><br />
    <%= f.text_field :service %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to "Back to Social Media Service list", services_path %>

