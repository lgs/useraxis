 
<% form_for @service.url  do |f| %>
  <%= f.error_messages %>
  <p>
    <%= image_tag("/images/sites_icons/lastfm.png") -%>
    <%= f.hidden_field :url, :value => 'http://www.lastfm.it/user/' %>
    <%= f.label :lastfm %>
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>
