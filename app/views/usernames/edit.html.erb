<h1>Editing username</h1>

<% form_for(@username) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :username %><br />
    <%= f.text_field :login_name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>
 
