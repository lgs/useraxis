<h1>Listing usernames</h1>

<table>
  <tr>
  </tr>

<% for username in @usernames %>
  <tr>
    <td><%= link_to 'Show', username %></td>
    <td><%= link_to 'Edit', edit_username_path(username) %></td>
    <td><%= link_to 'Destroy', username, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

