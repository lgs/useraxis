<h1>Editing service</h1>

<% form_for @service, :url => account_service_path(@service), :method => :put do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :service %><br />
    <%= f.text_field :service %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', account_service_path(@service) %> |
<%= link_to 'Back', account_services_path %>

