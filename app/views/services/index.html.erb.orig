<h1>Listing services</h1>

<table>
  <tr>
    <th>User name</th>
    <th>Service name</th>
    <th>URL</th>
  </tr>
<% for service in @services %>
  <tr>
    <td><%=h service.username %></td>
    <td><%=h service.servicename %></td>
    <td><%=h service.url %></td>
    <td><%= link_to 'Show', service %></td>
    <td><%= link_to 'Edit', edit_service_path(service) %></td>
    <td><%= link_to 'Destroy', service, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br /><%= link_to 'New service', new_service_path %>



