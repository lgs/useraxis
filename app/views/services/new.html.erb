<h1>New service</h1>

<% form_for(@service) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :servicename %>
    <%= f.text_field :servicename %> 
  </p>
  <p>
    <%= f.label :url %>
    <%= f.text_field :url %> 

    <!-- #%= f.hidden_field :account_id, :value => current_account.id % -->
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', services_url %>
